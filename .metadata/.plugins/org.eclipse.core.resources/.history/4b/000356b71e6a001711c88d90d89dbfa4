#include "Singleton.h"

Singleton::Singleton() {
	// TODO Auto-generated constructor stub
// Constructor is private
}
bool Singleton::createFLAG=false;
Singleton* Singleton::getInstance()
{
	Singleton* ptr;
	if(!createFLAG)
	{
		ptr= new Singleton;
	}

	return ptr;

}
Singleton::~Singleton() {
	// TODO Auto-generated destructor stub
}

