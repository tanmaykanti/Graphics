#include "Ellipset.h"
#include "Build.h"
Ellipse_t::Ellipse_t(float xc,float yc,float ac,float bc) {
	// TODO Auto-generated constructor stub
	x=xc;
	y=yc;
	a=ac;
	b=bc;

	theta=0.0;

}

float Ellipse_t::calculateR()
{
	float r;
	r = (a*b)/sqrt((b*cos(theta)*b*cos(theta)) + (a*sin(theta)*a*sin(theta)));
return r;
}

